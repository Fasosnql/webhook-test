{
  "name": "mygrove-components",
  "version": "1.6.0",
  "description": "Mygrove Components",
  "main": "index.js",
  "scripts": {
    "build:mobile": "rimraf dist-mobile && webpack --bail --progress --profile",
    "build:web": "rimraf dist && node setVariable.js --maxWidthMobile 0 --maxWidthIpad 0 && webpack --bail --progress --profile && node setVariable.js --maxWidthMobile 767 --maxWidthIpad 1024 --revert revert",
    "build": "npm run build:mobile && npm run build:web",
    "dev": "webpack-dev-server --history-api-fallback --inline --progress",
    "test:unit": "karma start karma.conf.js",
    "test:lint": "eslint .",
    "test": "npm run test:lint && npm run test:unit",
    "precommit": "npm run test",
    "release:major": "npm run build && git add -f dist/ dist-mobile/ && standard-version -a --no-verify -m \"release: %s\" --release-as major",
    "release:minor": "npm run build && git add -f dist/ dist-mobile/ && standard-version -a --no-verify -m \"release: %s\" --release-as minor",
    "release:patch": "npm run build && git add -f dist/ dist-mobile/ && standard-version -a --no-verify -m \"release: %s\" --release-as patch",
    "release": "npm run release:patch"
  },
  "repository": {
    "type": "git",
    "url": "git@git.fingoweb.com:fingowebcom/mygrove-components.git"
  },
  "author": "Fingoweb",
  "license": "ISC",
  "devDependencies": {
    "ajv": "^6.5.3",
    "babel-eslint": "^9.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "bootstrap": "^3.3.7",
    "eslint": "^5.5.0",
    "eslint-plugin-angular": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "file-system": "^2.2.2",
    "husky": "^0.14.3",
    "karma": "^3.0.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.3",
    "phantomjs-prebuilt": "^2.1.16",
    "ps-grab": "^1.0.1",
    "standard-version": "^4.4.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.6"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.20",
    "angular": "^1.7.4",
    "angular-animate": "1.7.4",
    "angular-cookies": "1.7.4",
    "angular-mocks": "^1.7.4",
    "angular-resource": "1.7.4",
    "angular-route": "1.7.4",
    "angular-touch": "1.7.4",
    "autolinker": "^1.7.1",
    "autoprefixer": "^9.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.2.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.2",
    "node-libs-browser": "^2.1.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.0",
    "webpack": "^4.17.1"
  }
}
